    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Python Debugger: Current File with Arguments",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/initial/src/run.py",
        "console": "integratedTerminal",
        "args": ["test","CheckSuite"],
        "cwd": "${workspaceFolder}/initial/src",
        "env": {
          "DEBUG_MODE": "true"
        }
      }
    ]
  }
  